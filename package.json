   "dependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^20.5.0",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "typeorm": "0.3.17",
      "reflect-metadata": "^0.1.13",
      "mysql": "^2.14.1"
   },
   "devDependencies": {
      "typescript": "4.5.2",
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10"
